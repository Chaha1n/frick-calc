<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.chaha1n" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>片手電卓</name>
    <description>
        フリック入力で使える画期的な電卓！
    </description>
    <author email="chahaaaaan@gmail.com" href="https://chaha1n.github.io/">
    </author>
    <content src="index.html" />
